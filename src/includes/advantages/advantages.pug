section.section.advantages
  div.container.advantages-container
    img.advantages-bg.advantags-bg--left.advantages-bg--left-dark(src="./images/planet-bg--dark.png")
    img.advantages-bg.advantages-bg--left.advantages-bg--left-light(src="./images/planet-bg--light.png")

    img.advantages-bg.advantags-bg--right.advantages-bg--right-dark(src="./images/planet-bg--dark.png")
    img.advantages-bg.advantages-bg--right.advantages-bg--right-light(src="./images/planet-bg--light.png")

    h2.section-title.advantages-title
      span.primary-text-fill Ready to use
    ul.advantages-list
      each item in advantages
        li.advantages-item
          a.advantages-item--link(href=item.link) 
            div.advantages-item--img!= item.img

            h4.advantages-item--title
              span.primary-text-fill= item.title
            p.advantages-item--description= item.description